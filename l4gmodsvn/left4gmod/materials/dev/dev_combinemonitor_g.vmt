Unlittwotexture

{
$basetexture _rt_Camera
"%tooltexture" "dev/dev_monitor"
$surfaceprop glass
$additive 1

$texture2 "dev/dev_combinescanline"

Proxies
{
Sine
{
resultVar "$color[1]"
sineperiod 1
sinemin .1
sinemax 0
}
Sine
{
resultVar "$color[2]"
sineperiod .01
sinemin .1
sinemax 0
}
TextureScroll
{
texturescrollvar $texture2transform
texturescrollrate .06
texturescrollangle -90
}
}
}
