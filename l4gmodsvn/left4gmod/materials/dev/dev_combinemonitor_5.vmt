Unlittwotexture

{
$basetexture _rt_Camera
"%tooltexture" "dev/dev_monitor"
$surfaceprop glass
$additive 1

$texture2 "dev/dev_combinescanline"

Proxies
{
Sine
{
resultVar "$color[0]"
sineperiod .1
sinemin .2
sinemax .21
}
Sine
{
resultVar "$color[1]"
sineperiod .1
sinemin .2
sinemax .21
}
Sine
{
resultVar "$color[2]"
sineperiod .1
sinemin .2
sinemax .21
}
TextureScroll
{
texturescrollvar $texture2transform
texturescrollrate .06
texturescrollangle -90
}
}
}
