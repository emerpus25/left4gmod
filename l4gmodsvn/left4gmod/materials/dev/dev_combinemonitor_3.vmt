Unlittwotexture

{
$basetexture _rt_Camera
"%tooltexture" "dev/dev_monitor"
$surfaceprop glass
$additive 1

$texture2 "dev/dev_combinescanline"


"srgb?$color2" "[2.5 2.5 2.5]"
Proxies
{
Sine
{
resultVar "$color[0]"
sineperiod .1
sinemin .4
sinemax .41
}
Sine
{
resultVar "$color[1]"
sineperiod .1
sinemin .4
sinemax .41
}
Sine
{
resultVar "$color[2]"
sineperiod .1
sinemin .4
sinemax .41
}
TextureScroll
{
texturescrollvar $texture2transform
texturescrollrate .06
texturescrollangle -90
}
}
}
